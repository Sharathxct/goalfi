{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "expo-health-kit": ["node_modules/expo-health-kit"]
    },
    "baseUrl": "."
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "nativewind-env.d.ts",
    "expo-env.d.ts",
    "babel.config.js",
    "metro.config.js",
    "tailwind.config.ts"
  ]
}
